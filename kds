        <script>
            function OnSelectionChange()
            {
                var city = document.getElementById("mySelect").value;
                console.log(city);
                document.getElementById("Providence").style.display = "none";
                document.getElementById("Cranston").style.display = "none";
                document.getElementById("Lincoln").style.display = "none";
                document.getElementById("Johnston").style.display = "none";
                document.getElementById("Warwick").style.display = "none";
                document.getElementById("WestWarwick").style.display = "none";
                document.getElementById("Coventry").style.display = "none";
                document.getElementById("EastProvidence").style.display = "none";
                document.getElementById("NorthProvidence").style.display = "none";
                document.getElementById("Pawtucket").style.display = "none";
                document.getElementById("CentralFalls").style.display = "none";
                document.getElementById("Cumberland").style.display = "none";
                document.getElementById("CollegeHills").style.display = "none";
                document.getElementById("FoxPoint").style.display = "none";
                document.getElementById("Woonsoket").style.display = "none";
                
                if(city == "providence"){
                    document.getElementById("Providence").style.display = "block";
                }
                else if(city == "cranston"){
                    document.getElementById("Cranston").style.display = "block";
                }
                else if(city == "lincoln"){
                    document.getElementById("Lincoln").style.display = "block";
                }
                else if(city == "johnston"){
                    document.getElementById("Johnston").style.display = "block";
                }
                else if(city == "warwick"){
                    document.getElementById("Warwick").style.display = "block";
                }
                else if(city == "westwarwick"){
                    document.getElementById("WestWarwick").style.display = "block";
                }
                else if(city == "coventry"){
                    document.getElementById("Coventry").style.display = "block";
                }
                else if(city == "eastprovidence"){
                    document.getElementById("EastProvidence").style.display = "block";
                }
                else if(city == "northprovidence"){
                    document.getElementById("NorthProvidence").style.display = "block";
                }
                else if(city == "pawtucket"){
                    document.getElementById("Pawtucket").style.display = "block";
                }
                else if(city == "centralfalls"){
                    document.getElementById("CentralFalls").style.display = "block";
                }
                else if(city == "cumberland"){
                    document.getElementById("Cumberland").style.display = "block";
                }
                else if(city == "collegehills"){
                    document.getElementById("CollegeHills").style.display = "block";
                }
                else if(city == "foxpoint"){
                    document.getElementById("FoxPoint").style.display = "block";
                }
                else if(city == "woonsoket"){
                    document.getElementById("Woonsoket").style.display = "block";
                }

                //alert("OK IT WORKS");
            }
        </script>
                
